/* pages/wether/wether.wxss */

@font-face {
    font-family: 'icomoon';
    src: url(data:font/truetype;charset=utf-8;base64,) format('truetype');
    font-weight: normal;
    font-style: normal;
}


page {
  background-color: rgba(134, 247, 209, 0.664);
}

.container {
  height:100%;
  display:flex;
  flex-direction:column;
  align-items:center;
  justify-content:space-between;
  padding:160rpx 0;
  box-sizing:border-box;
  background:#999 linear-gradient(rgba(48, 41, 41, 0.7), transparent);
  position: relative;
}

.add {
  position: fixed;
  left: 16rpx;
  top: 16rpx;
  z-index: 2
}

.chooseAddTap {
  height: 626rpx;
  width: 100%;
  position: fixed;
  top: 0;
  left: 0;
  background-color: white;
  z-index: 999;
  opacity: 0.8;
}

.picker{
  position: relative;
  height: 30%;
  width: 100%;
  padding: 13px;
  background-color: white;
  top: 230rpx;
  left: 0;
  box-sizing: border-box;
}

.pickerItem {
  height: 100%;
  margin: 0 20rpx;
  display: inline-block;
  font-size: bold
}

.pickerIcon {
  position: absolute;
  top: 10rpx;
  right: 10rpx;
  z-index: 9;
}

.city {
  position: fixed;
  right: 10rpx;
  font-size: 32rpx;
  top: 16rpx;
  z-index: 2;
}

.shuaxin {
  position: fixed;
  right: 200rpx;
  top: 16rpx;
  font-size: 32rpx;
  z-index: 2;
}

.rotate {
  height: 32rpx;
  width: 32rpx;
  /* background-color: #ccc; */
  display: inline-block;
}


.cityspan {
  padding-right: 8rpx;
  font-family: "icomoon";
  color: #c03;
}

.temperature {
  font-size: 180rpx;
  position: relative;
  font-family: "Josefin Sans";
}

.leftTop {
  position: absolute;
  height: 60rpx;
  width: 110rpx;
  top: -10rpx;
  right: -120rpx;
}

.temperatureIcon {
  height: 24rpx;
  width: 24rpx;
  border-radius: 50%;
  border: 8rpx solid #fff;
  float: left;
  margin-right: 20rpx;
  margin-top: 6rpx;
}

.weatherDesc {
  float: left;
  font-size: 30rpx;
  line-height: 60rpx;
  margin-top: 4rpx;
}

.tag {
  width: 60%;
  height: 80rpx;
}

.pm {
  float: left;
  font-size: 30rpx;
}

.airQuality {
  float: left;
  height: 32rpx;
  width: 45rpx;
  border-radius: 4rpx;
  color: #fff; 
  font-weight: bold;
  font-size: 28rpx;
  margin: 8rpx 0 0 8rpx;
  line-height: 30rpx;
  text-align: center;
}

.temperaturerange {
  float: right;
  font-size: 30rpx;
}

.wind {
  font-size: 30rpx;  
  background-clip: #555;
}

.allList {
  position: absolute;
  height: 32rpx;
  bottom: 32rpx;
  left: 50%;
  transform: translateX(-50%);
}

.listChoose {
  height: 22rpx;
  width: 22rpx;
  border-radius: 50%;
  border: 2rpx solid white;
  background-color: #666;
  box-sizing: border-box;
  display: block;
  float: left;
  margin: 5rpx 15rpx;
}

.choosefirst {
  float: left;
  margin: 0rpx 15rpx;
  height: 22rpx;
  display: block;
  width: 22rpx;
}

.listChooseStyle {
  border: 10rpx solid rgba(255, 255, 255, 0.8);
  height: 32rpx;
  width: 32rpx;
  margin: 0 12rpx;
}

.originalweather {
  display: flex;
  padding: 10rpx 10rpx 0;
  flex-wrap: nowrap;
  height: 600rpx;
  box-sizing: border-box;
}

.item {
  flex-grow: 1;
  width: 20%;
  margin: 0 10rpx;
  background-clip: #ccc;
}

.picture {
  width: 90%;
  height: 120rpx;
  margin: 16rpx 0;
}

.detail {
  height: 210rpx;
  font-size: 28rpx;
}

.list {
  margin: 18rpx 6rpx;
}