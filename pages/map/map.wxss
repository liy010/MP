/* pages/map/map.wxss */

button {
  height: 70rpx;
  font-size: 32rpx;
  line-height: 70rpx;
  width: 40%;
  margin: 0 auto;
  background: rgb(15, 168, 15) linear-gradient(hsla(0, 0%, 100%, .2), transparent);
  border: 1px solid #ddd;
}
.mapStyle,
.canvas1,
.canvas2,
.canvas3,
.canvas4 {
  margin: 20px auto 8rpx;
  border: 1px solid #ccc;
}

view {
  color: rgb(17, 44, 31);
}

span {
  color: rgb(4, 245, 153);
}